      "TextList": [
        {
          "Line": 200,
          "OriginalText": "There\u0027s still some monsters left. ",
          "TranslatedText": null,
          "Colour": null
        },
        {
          "Line": 204,
          "OriginalText": "Is my\nsister gonna be OK?",
          "TranslatedText": null,
          "Colour": null
        }
      ]
    },
    {
      "EntryName": "PairAtacked01",
      "TextList": [
        {
          "Line": 213,
          "OriginalText": "Don\u0027t talk to me! Go help my sister!",
          "TranslatedText": null,
          "Colour": null
        }
      ]
    },
    {
      "EntryName": "PairFailed00",
      "TextList": [
        {
          "Line": 222,
          "OriginalText": "Meghyn, wake up... ",
          "TranslatedText": null,
          "Colour": null
        },
        {
          "Line": 226,
          "OriginalText": "Come on! Meghyn!",
