    {
      "EntryName": "Talk_08",
      "NPC": "Maike",
      "Choices": [
        5,
        1
      ],
      "TextList": [
        {
          "Line": 389,
          "OriginalText": "You want to know if I\u0027ve seen a\nsuspicious-looking male merchant?\n\n",
          "TranslatedText": null
        },
        {
          "Line": 395,
          "OriginalText": "Suspicious, huh...",
          "TranslatedText": null
        },
        {
          "Line": 399,
          "OriginalText": "\nHmmmm...",
          "TranslatedText": null
        },
        {
          "Line": 403,
          "OriginalText": "\n\n",
          "TranslatedText": null
        },
        {
          "Line": 409,
          "OriginalText": "Ahh, I dunno. It\u0027s hard to say. I spend all\nmy time right here, and I don\u0027t know how\nI\u0027d tell if someone was a merchant...\nAnyway, there are Hylian merchants here,\ntoo. Why don\u0027t you go ask one of them?",
          "TranslatedText": null
        }
      ]
