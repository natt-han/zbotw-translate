    },
    {
      "EntryName": "Talk_03",
      "NPC": "Essa",
      "TextList": [
        {
          "Line": 101,
          "OriginalText": "All right! I\u0027m gonna do my best at my\npart-time job!\n\nIt\u0027s actually really tough taking care of\nthe ",
          "TranslatedText": null,
          "Length": 100
        },
        {
          "Line": 105,
          "OriginalText": "sand seals",
          "TranslatedText": null,
          "Colour": "blue",
          "Length": 10
        },
        {
          "Line": 108,
          "OriginalText": ",",
          "TranslatedText": null,
          "Length": 1
        },
        {
          "Line": 112,
          "OriginalText": " but seeing how happy\nthey are when their food is put out...\nAlthough, I do sometimes put out too\nmuch food...\n\n",
          "TranslatedText": null,
          "Length": 112
        },
        {
          "Line": 118,
          "OriginalText": "I\u0027m so exhausted when the day is over.",
          "TranslatedText": null,
          "Length": 38
        }
      ]
    },
