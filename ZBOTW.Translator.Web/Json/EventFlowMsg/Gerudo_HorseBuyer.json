    {
      "EntryName": "Near_01",
      "NPC": "Zyle",
      "TextList": [
        {
          "Line": 36,
          "OriginalText": "Hey... Hey... Hey...",
          "TranslatedText": "เฮ้... เฮ้... เฮ้..."
        }
