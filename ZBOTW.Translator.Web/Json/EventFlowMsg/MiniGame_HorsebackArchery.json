      "TextList": [
        {
          "Line": 368,
          "OriginalText": "Tell you what...",
          "TranslatedText": null,
          "Colour": null
        },
        {
          "Line": 372,
          "OriginalText": " You break ",
          "TranslatedText": null,
          "Colour": null
        },
        {
