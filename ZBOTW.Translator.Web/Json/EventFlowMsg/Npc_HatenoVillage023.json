    {
      "EntryName": "talk15",
      "TextList": [
        {
          "Line": 214,
          "OriginalText": "The village has grown to be quite a lively\nplace. I suppose that\u0027s on account of\nthings bein\u0027 so peaceful in general. ",
          "TranslatedText": null,
          "Colour": null
        }
      ]
