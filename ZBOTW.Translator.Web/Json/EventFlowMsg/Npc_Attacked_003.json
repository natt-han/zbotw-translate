    },
    {
      "EntryName": "SavedLv3_00",
      "TextList": [
        {
          "Line": 243,
          "OriginalText": "It blows my mind that anyone bothered\nto save a sad sack like me.",
          "TranslatedText": null,
          "Colour": null
