    {
      "EntryName": "Com_Talk_33",
      "NPC": "Rola",
      "Choices": [
        5500,
        5501
      ],
      "TextList": [
        {
          "Line": 317,
          "OriginalText": "Just one, right?",
          "TranslatedText": "ชิ้นเดียวเหรอ? ",
          "Length": 16
