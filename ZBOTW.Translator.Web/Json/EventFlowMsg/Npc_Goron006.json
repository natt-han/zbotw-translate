    {
      "EntryName": "talk112",
      "NPC": "Bludo",
      "Choices": [
        1,
        2
      ],
      "TextList": [
        {
          "Line": 279,
          "OriginalText": "Oh, it\u0027s you...\n\n\nWho are you anyway?",
          "TranslatedText": null
