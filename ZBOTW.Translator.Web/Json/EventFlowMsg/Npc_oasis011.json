        {
          "Line": 136,
          "OriginalText": "\n\n",
          "TranslatedText": null
        },
        {
          "Line": 142,
          "OriginalText": "It looks like he went away somewhere...\n\n\n",
          "TranslatedText": null
        },
        {
          "Line": 148,
          "OriginalText": "He must have left because everyone kept\nsaying he was a bad boy all the time!",
          "TranslatedText": null
        }
      ]
    },
