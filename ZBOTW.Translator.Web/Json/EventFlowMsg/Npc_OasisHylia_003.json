    {
      "EntryName": "Talk_02",
      "NPC": "Tauma",
      "TextList": [
        {
          "Line": 131,
          "OriginalText": "Huh? Wait, did you already...\nBut why? How?!",
          "TranslatedText": null,
          "Length": 44
        },
        {
          "Line": 135,
          "OriginalText": "\n\n",
          "TranslatedText": null,
          "Length": 2
        },
        {
          "Line": 141,
          "OriginalText": "I\u0027ve been here forever—she\u0027s not taking\npeople in the order that they arrived!",
          "TranslatedText": null,
          "Length": 78
        },
        {
          "Line": 145,
          "OriginalText": "\n\n",
          "TranslatedText": null,
          "Length": 2
        },
        {
          "Line": 151,
          "OriginalText": "Wait! I can\u0027t get angry... I won\u0027t\nget angry...",
          "TranslatedText": null,
          "Length": 47
