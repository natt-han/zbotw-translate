      "TextList": [
        {
          "Line": 547,
          "OriginalText": "Thank you for listening to",
          "TranslatedText": null,
          "Length": 26
        },
        {
          "Line": 551,
          "OriginalText": " my teacher\u0027s\nsong.\n\nIf you want to hear the song again,\nyou\u0027ll need to come back at nighttime.\n\n",
          "TranslatedText": null,
          "Length": 97
        },
        {
          "Line": 557,
          "OriginalText": "That\u0027s the only time my kids will leave\nme alone...",
          "TranslatedText": null,
          "Length": 51
        }
      ]
    },
