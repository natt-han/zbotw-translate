        {
          "Line": 208,
          "OriginalText": "Medoh",
          "TranslatedText": null
        },
        {
          "Line": 211,
          "OriginalText": ", I suggested we\nlet you borrow the ",
          "TranslatedText": null
        },
        {
          "Line": 215,
          "OriginalText": "Great Eagle Bow",
          "TranslatedText": null
        },
        {
