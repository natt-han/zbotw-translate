    {
      "EntryName": "MSG_Firo_01_1",
      "TextList": [
        {
          "Line": 131,
          "OriginalText": "A traveler who got lost in ",
          "TranslatedText": null,
          "Colour": null
        },
        {
          "Line": 135,
          "OriginalText": "Faron Woods",
          "TranslatedText": null,
          "Colour": "blue"
        },
