      "TextList": [
        {
          "Line": 122,
          "OriginalText": "Playing dumb, eh?!",
          "TranslatedText": null,
          "Colour": null
        },
        {
          "Line": 126,
          "OriginalText": " Nice try,\nbut you cannot fool me,\nChampion Link!",
          "TranslatedText": null,
          "Colour": null
        },
        {
          "Line": 130,
          "OriginalText": "\nYou were unable to protect Lady Mipha",
          "TranslatedText": null,
          "Colour": null
        },
        {
          "Line": 134,
          "OriginalText": "\nfrom ",
          "TranslatedText": null,
          "Colour": null
