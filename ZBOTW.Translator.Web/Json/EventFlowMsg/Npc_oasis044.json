    {
      "EntryName": "Talk_09",
      "NPC": "Spera",
      "TextList": [
        {
          "Line": 519,
          "OriginalText": "Sav\u0027saaba...",
          "TranslatedText": null
        },
        {
          "Line": 523,
          "OriginalText": "\nThis is a city that doesn\u0027t sleep,\nso this store is always open.\nUnfortunately, being so busy means I\ndon\u0027t often make it home to my\nhusband...",
          "TranslatedText": null
        }
      ]
