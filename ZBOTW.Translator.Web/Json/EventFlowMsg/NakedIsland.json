    },
    {
      "EntryName": "talk01",
      "TextList": [
        {
          "Line": 20,
          "OriginalText": "You\u0027ve done well to complete the trial.\nI will now return your things, as\nI promised.",
          "TranslatedText": null,
          "Colour": null
        }
      ]
    },
    {
      "EntryName": "talk02",
      "TextList": [
        {
          "Line": 24,
          "OriginalText": "Give up on the challenge?",
          "TranslatedText": null,
          "Colour": null
