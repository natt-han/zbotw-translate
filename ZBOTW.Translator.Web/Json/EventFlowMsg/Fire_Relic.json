{
  "FileName": "Fire_Relic",
  "EntryList": [
    {
      "EntryName": "0003",
      "TextList": [
        {
          "Line": 8,
          "OriginalText": "Lookin\u0027 for Yunobo!",
          "TranslatedText": null
        }
      ]
    },
    {
      "EntryName": "0012",
      "TextList": [
        {
          "Line": 12,
          "OriginalText": "I won\u0027t give up.",
          "TranslatedText": null
        }
      ]
    },
    {
      "EntryName": "0019",
      "TextList": [
        {
          "Line": 16,
          "OriginalText": "I wanna board Rudania.",
          "TranslatedText": null
        }
      ]
    },
    {
      "EntryName": "0033",
      "TextList": [
        {
          "Line": 20,
          "OriginalText": "With a cannon.",
          "TranslatedText": null
        }
      ]
    },
    {
      "EntryName": "0034",
      "TextList": [
        {
          "Line": 24,
          "OriginalText": "Wait a minute!",
          "TranslatedText": null
        }
      ]
    },
    {
      "EntryName": "0035",
      "TextList": [
        {
          "Line": 28,
          "OriginalText": "A wink!",
          "TranslatedText": null
        }
      ]
    },
    {
      "EntryName": "0036",
      "TextList": [
        {
          "Line": 32,
          "OriginalText": "A whistle!",
          "TranslatedText": null
        }
      ]
    },
    {
      "EntryName": "0037",
      "TextList": [
        {
          "Line": 36,
          "OriginalText": "Mind powers!",
          "TranslatedText": null
        }
      ]
    },
    {
      "EntryName": "0038",
      "TextList": [
        {
          "Line": 40,
          "OriginalText": "I give up.",
          "TranslatedText": null
        }
      ]
    },
    {
      "EntryName": "0039",
      "TextList": [
        {
          "Line": 44,
          "OriginalText": "Are you sure?!",
          "TranslatedText": null
        }
      ]
    },
    {
      "EntryName": "0040",
      "TextList": [
        {
          "Line": 48,
          "OriginalText": "He\u0027s not coming.",
          "TranslatedText": null
        }
      ]
    },
    {
      "EntryName": "0041",
      "TextList": [
        {
          "Line": 52,
          "OriginalText": "His back hurts.",
          "TranslatedText": null
        }
      ]
    },
    {
      "EntryName": "near01",
      "TextList": [
        {
          "Line": 56,
          "OriginalText": "Oh, no...goro...",
          "TranslatedText": null
        }
      ]
    },
    {
      "EntryName": "near04",
      "TextList": [
        {
          "Line": 60,
          "OriginalText": "...",
          "TranslatedText": null
        }
      ]
    },
    {
      "EntryName": "near05",
      "TextList": [
        {
          "Line": 64,
          "OriginalText": "Hey!",
          "TranslatedText": null
        }
      ]
    },
    {
      "EntryName": "near06",
      "TextList": [
        {
          "Line": 68,
          "OriginalText": "Over here!",
          "TranslatedText": null
        }
      ]
    },
    {
      "EntryName": "near08",
      "TextList": [
        {
          "Line": 72,
          "OriginalText": "Phew...",
          "TranslatedText": null
        }
      ]
    },
    {
      "EntryName": "talk00",
      "TextList": [
        {
          "Line": 81,
          "OriginalText": "But what brought you all the way\nout here to begin with?",
          "TranslatedText": null
        }
      ]
    },
    {
      "EntryName": "talk01",
      "TextList": [
        {
          "Line": 93,
          "OriginalText": "Oh!",
          "TranslatedText": null
        },
        {
          "Line": 97,
          "OriginalText": " So that\u0027s it!",
          "TranslatedText": null
        },
        {
          "Line": 101,
          "OriginalText": " ",
          "TranslatedText": null
        },
        {
          "Line": 105,
          "OriginalText": "Boss ",
          "TranslatedText": null
        },
        {
          "Line": 108,
          "OriginalText": "told you to come\nfind me!\n\n",
          "TranslatedText": null
        },
        {
          "Line": 114,
          "OriginalText": "Phew, thank goodness. ",
          "TranslatedText": null
        },
        {
          "Line": 118,
          "OriginalText": "In any case,\nI sure am glad you saved me, goro!",
          "TranslatedText": null
        }
      ]
    },
    {
      "EntryName": "talk100",
      "TextList": [
        {
          "Line": 127,
          "OriginalText": "I think your telepathic signals may be\ncrossed or malfunctioning, goro.",
          "TranslatedText": null
        },
        {
          "Line": 131,
          "OriginalText": "\nI don\u0027t seem to be reading you.",
          "TranslatedText": null
        }
      ]
    },
    {
      "EntryName": "talk102",
      "TextList": [
        {
          "Line": 140,
          "OriginalText": "I\u0027m tellin\u0027 you, I won\u0027t be able to see\na wink from that far away, goro!",
          "TranslatedText": null
        }
      ]
    },
    {
      "EntryName": "talk103",
      "TextList": [
        {
          "Line": 149,
          "OriginalText": "Hey! ",
          "TranslatedText": null
        },
        {
          "Line": 153,
          "OriginalText": "Where you going, goro?!",
          "TranslatedText": null
        }
      ]
    },
    {
      "EntryName": "talk104",
      "TextList": [
        {
          "Line": 162,
          "OriginalText": "You really wanna give up tryin\u0027 to\nboard ",
          "TranslatedText": null
        },
        {
          "Line": 166,
          "OriginalText": "Rudania",
          "TranslatedText": null
        },
        {
          "Line": 169,
          "OriginalText": ", goro?",
          "TranslatedText": null
        }
      ]
    },
    {
      "EntryName": "talk105",
      "TextList": [
        {
          "Line": 186,
          "OriginalText": "OK then, goro. ",
          "TranslatedText": null
        },
        {
          "Line": 190,
          "OriginalText": "Let\u0027s get goin\u0027 again!",
          "TranslatedText": null
        }
      ]
    },
    {
      "EntryName": "talk106",
      "TextList": [
        {
          "Line": 199,
          "OriginalText": "Oh, OK. I get it. ",
          "TranslatedText": null
        },
        {
          "Line": 203,
          "OriginalText": "When you\u0027re ready,\nplease just come back here, goro.",
          "TranslatedText": null
        }
      ]
    },
    {
      "EntryName": "talk108",
      "TextList": [
        {
          "Line": 215,
          "OriginalText": "Boss ",
          "TranslatedText": null
        },
        {
          "Line": 218,
          "OriginalText": "is over here, goro!",
          "TranslatedText": null
        },
        {
          "Line": 222,
          "OriginalText": "\nCome with me!",
          "TranslatedText": null
        }
      ]
    },
    {
      "EntryName": "talk109",
      "TextList": [
        {
          "Line": 234,
          "OriginalText": "Boss ",
          "TranslatedText": null
        },
        {
          "Line": 237,
          "OriginalText": "has somethin\u0027 he wants to talk to\nyou about. ",
          "TranslatedText": null
        },
        {
          "Line": 241,
          "OriginalText": "I wonder what it is, goro...",
          "TranslatedText": null
        }
      ]
    },
    {
      "EntryName": "talk110",
      "TextList": [
        {
          "Line": 250,
          "OriginalText": "OK! ",
          "TranslatedText": null
        },
        {
          "Line": 254,
          "OriginalText": "Let\u0027s hear what ",
          "TranslatedText": null
        },
        {
          "Line": 258,
          "OriginalText": "Boss",
          "TranslatedText": null
        },
        {
          "Line": 261,
          "OriginalText": " has\nto say, goro!",
          "TranslatedText": null
        }
      ]
    },
    {
      "EntryName": "talk111",
      "TextList": [
        {
          "Line": 270,
          "OriginalText": "What did ",
          "TranslatedText": null
        },
        {
          "Line": 274,
          "OriginalText": "Boss ",
          "TranslatedText": null
        },
        {
          "Line": 277,
          "OriginalText": "want, goro?",
          "TranslatedText": null
        },
        {
          "Line": 281,
          "OriginalText": "\nYou should ask him directly!",
          "TranslatedText": null
        }
      ]
    },
    {
      "EntryName": "talk114",
      "TextList": [
        {
          "Line": 290,
          "OriginalText": "OK. Let\u0027s hurry up and prepare\nthe ",
          "TranslatedText": null
        },
        {
          "Line": 294,
          "OriginalText": "cannon",
          "TranslatedText": null
        },
        {
          "Line": 297,
          "OriginalText": ", goro!",
          "TranslatedText": null
        }
      ]
    },
    {
      "EntryName": "talk115",
      "TextList": [
        {
          "Line": 311,
          "OriginalText": "Yeesh... ",
          "TranslatedText": null
        },
        {
          "Line": 315,
          "OriginalText": "My knees are shaking, goro...",
          "TranslatedText": null
        }
      ]
    },
    {
      "EntryName": "talk116",
      "TextList": [
        {
          "Line": 324,
          "OriginalText": "We need to tread lightly, goro...",
          "TranslatedText": null
        }
      ]
    },
    {
      "EntryName": "talk117",
      "TextList": [
        {
          "Line": 333,
          "OriginalText": "Next ",
          "TranslatedText": null
        },
        {
          "Line": 337,
          "OriginalText": "cannon",
          "TranslatedText": null
        },
        {
          "Line": 340,
          "OriginalText": "... ",
          "TranslatedText": null
        },
        {
          "Line": 344,
          "OriginalText": "Next ",
          "TranslatedText": null
        },
        {
          "Line": 348,
          "OriginalText": "cannon",
          "TranslatedText": null
        },
        {
          "Line": 351,
          "OriginalText": "...",
          "TranslatedText": null
        }
      ]
    },
    {
      "EntryName": "talk118",
      "TextList": [
        {
          "Line": 360,
          "OriginalText": "OK. It\u0027s not too terribly different from\nusual so far, goro.",
          "TranslatedText": null
        }
      ]
    },
    {
      "EntryName": "talk119",
      "TextList": [
        {
          "Line": 369,
          "OriginalText": "OK. ",
          "TranslatedText": null
        },
        {
          "Line": 373,
          "OriginalText": "We just need to keep going, goro.",
          "TranslatedText": null
        }
      ]
    },
    {
      "EntryName": "talk120",
      "TextList": [
        {
          "Line": 382,
          "OriginalText": "Just a little more ",
          "TranslatedText": null
        },
        {
          "Line": 386,
          "OriginalText": "and we can drive\nit off, goro...",
          "TranslatedText": null
        }
      ]
    },
    {
      "EntryName": "talk121",
      "TextList": [
        {
          "Line": 401,
          "OriginalText": "Yaah! ",
          "TranslatedText": null
        },
        {
          "Line": 405,
          "OriginalText": "Someone, please help me!",
          "TranslatedText": null
        }
      ]
    },
    {
      "EntryName": "talk122",
      "TextList": [
        {
          "Line": 417,
          "OriginalText": "WOW! ",
          "TranslatedText": null
        },
        {
          "Line": 421,
          "OriginalText": "You\u0027re crazy strong, goro!",
          "TranslatedText": null
        },
        {
          "Line": 425,
          "OriginalText": "\nYou saved me again!",
          "TranslatedText": null
        }
      ]
    },
    {
      "EntryName": "talk123",
      "TextList": [
        {
          "Line": 434,
          "OriginalText": "The problem is...good ol\u0027 Boss raised the\n",
          "TranslatedText": null
        },
        {
          "Line": 438,
          "OriginalText": "Bridge of Eldin",
          "TranslatedText": null
        },
        {
          "Line": 444,
          "OriginalText": " to make sure that mean\nDivine Beast couldn\u0027t get over here.\nIn order to get to where ",
          "TranslatedText": null
        },
        {
          "Line": 448,
          "OriginalText": "Rudania",
          "TranslatedText": null
        },
        {
          "Line": 451,
          "OriginalText": " is,",
          "TranslatedText": null
        },
        {
          "Line": 455,
          "OriginalText": "\nwe\u0027ll have to find a way to get the\n",
          "TranslatedText": null
        },
        {
          "Line": 459,
          "OriginalText": "Bridge of Eldin ",
          "TranslatedText": null
        },
        {
          "Line": 462,
          "OriginalText": "back down, goro.\n",
          "TranslatedText": null
        },
        {
          "Line": 466,
          "OriginalText": "Boss ",
          "TranslatedText": null
        },
        {
          "Line": 469,
          "OriginalText": "would normally use the ",
          "TranslatedText": null
        },
        {
          "Line": 473,
          "OriginalText": "cannon",
          "TranslatedText": null
        },
        {
          "Line": 476,
          "OriginalText": "\nto lower it... ",
          "TranslatedText": null
        },
        {
          "Line": 480,
          "OriginalText": "Ugh, we can\u0027t get across\nunless we figure somethin\u0027 out!",
          "TranslatedText": null
        }
      ]
    },
    {
      "EntryName": "talk13",
      "TextList": [
        {
          "Line": 489,
          "OriginalText": "Hmm...\n\n\nHMMM...\n\n\n",
          "TranslatedText": null
        },
        {
          "Line": 496,
          "OriginalText": "Now that I\u0027ve gotten a good look...\nWhoa! It\u0027s a Hylian!",
          "TranslatedText": null
        },
        {
          "Line": 502,
          "OriginalText": "\n\n",
          "TranslatedText": null
        },
        {
          "Line": 508,
          "OriginalText": "Were you the one who broke through\nthat crag to rescue me, goro?\nThanks, brother!",
          "TranslatedText": null
        }
      ]
    },
    {
      "EntryName": "talk143",
      "TextList": [
        {
          "Line": 517,
          "OriginalText": "Eek! Help meeee!\nI\u0027m scared, goro!",
          "TranslatedText": null
        }
      ]
    },
    {
      "EntryName": "talk144",
      "TextList": [
        {
          "Line": 529,
          "OriginalText": "Uh-oh. ",
          "TranslatedText": null
        },
        {
          "Line": 533,
          "OriginalText": "Yikes, goro. ",
          "TranslatedText": null
        },
        {
          "Line": 537,
          "OriginalText": "We can\u0027t keep goin\u0027\nwith those ",
          "TranslatedText": null
        },
        {
          "Line": 541,
          "OriginalText": "sentries ",
          "TranslatedText": null
        },
        {
          "Line": 544,
          "OriginalText": "in the way...\n\n",
          "TranslatedText": null
        },
        {
          "Line": 550,
          "OriginalText": "What should we do?",
          "TranslatedText": null
        },
        {
          "Line": 554,
          "OriginalText": " How can we\npossibly move forward?\n\n",
          "TranslatedText": null
        },
        {
          "Line": 560,
          "OriginalText": "There must be a way, goro...",
          "TranslatedText": null
        }
      ]
    },
    {
      "EntryName": "talk148",
      "TextList": [
        {
          "Line": 569,
          "OriginalText": "The name\u0027s ",
          "TranslatedText": null
        },
        {
          "Line": 573,
          "OriginalText": "Yunobo",
          "TranslatedText": null
        },
        {
          "Line": 576,
          "OriginalText": ". Everyone calls me...\n",
          "TranslatedText": null
        },
        {
          "Line": 583,
          "OriginalText": "Yunobo",
          "TranslatedText": null
        },
        {
          "Line": 586,
          "OriginalText": ". ",
          "TranslatedText": null
        },
        {
          "Line": 590,
          "OriginalText": "Makes sense, goro.",
          "TranslatedText": null
        }
      ]
    },
    {
      "EntryName": "talk149",
      "TextList": [
        {
          "Line": 599,
          "OriginalText": "I came to get some painkillers for Boss,",
          "TranslatedText": null
        },
        {
          "Line": 603,
          "OriginalText": "\nbut a magma bomb caused a rock slide,\nsealing the entrance, goro.",
          "TranslatedText": null
        }
      ]
    },
    {
      "EntryName": "talk151",
      "TextList": [
        {
          "Line": 612,
          "OriginalText": "By the way... ",
          "TranslatedText": null
        },
        {
          "Line": 616,
          "OriginalText": "How did you break those\nrocks at the entrance, goro?",
          "TranslatedText": null
        }
      ]
    },
    {
      "EntryName": "talk152",
      "TextList": [
        {
          "Line": 631,
          "OriginalText": "Huh?",
          "TranslatedText": null
        },
        {
          "Line": 635,
          "OriginalText": " You used ",
          "TranslatedText": null
        },
        {
          "Line": 639,
          "OriginalText": "Boss",
          "TranslatedText": null
        },
        {
          "Line": 642,
          "OriginalText": "\u0027s cannon?!",
          "TranslatedText": null
        },
        {
          "Line": 646,
          "OriginalText": "\n\n\n",
          "TranslatedText": null
        },
        {
          "Line": 652,
          "OriginalText": "That thing is so difficult to use.",
          "TranslatedText": null
        },
        {
          "Line": 656,
          "OriginalText": " I thought\nonly ",
          "TranslatedText": null
        },
        {
          "Line": 660,
          "OriginalText": "Boss ",
          "TranslatedText": null
        },
        {
          "Line": 663,
          "OriginalText": "could handle it!",
          "TranslatedText": null
        }
      ]
    },
    {
      "EntryName": "talk153",
      "TextList": [
        {
          "Line": 678,
          "OriginalText": "Oops, I nearly forgot! ",
          "TranslatedText": null
        },
        {
          "Line": 682,
          "OriginalText": "This isn\u0027t the time to chat...",
          "TranslatedText": null
        },
        {
          "Line": 686,
          "OriginalText": "\nI need to deliver these painkillers as soon\nas possible!",
          "TranslatedText": null
        },
        {
          "Line": 692,
          "OriginalText": "\n",
          "TranslatedText": null
        },
        {
          "Line": 698,
          "OriginalText": "Sorry, I\u0027ve gotta run! ",
          "TranslatedText": null
        },
        {
          "Line": 702,
          "OriginalText": "But do come by and\nsee the ",
          "TranslatedText": null
        },
        {
          "Line": 706,
          "OriginalText": "Boss ",
          "TranslatedText": null
        },
        {
          "Line": 709,
          "OriginalText": "later, OK? ",
          "TranslatedText": null
        },
        {
          "Line": 713,
          "OriginalText": "He may be gruff,\nbut he always repays a favor.\nI\u0027m sure there\u0027s a ",
          "TranslatedText": null
        },
        {
          "Line": 717,
          "OriginalText": "reward ",
          "TranslatedText": null
        },
        {
          "Line": 720,
          "OriginalText": "in it for you.\nOK then! Good-bye for now, goro!",
          "TranslatedText": null
        }
      ]
    },
    {
      "EntryName": "talk154",
      "TextList": [
        {
          "Line": 729,
          "OriginalText": "After we went through all that trouble to\nget them...",
          "TranslatedText": null
        },
        {
          "Line": 733,
          "OriginalText": "the painkillers didn\u0027t even\nend up working, goro.\nWell, if ",
          "TranslatedText": null
        },
        {
          "Line": 737,
          "OriginalText": "Boss ",
          "TranslatedText": null
        },
        {
          "Line": 740,
          "OriginalText": "can\u0027t help, there\u0027s nothin\u0027\nelse we can do.",
          "TranslatedText": null
        },
        {
          "Line": 744,
          "OriginalText": " I suppose I\u0027ll just head\nback now...",
          "TranslatedText": null
        }
      ]
    },
    {
      "EntryName": "talk155",
      "TextList": [
        {
          "Line": 756,
          "OriginalText": "Huh? You\u0027re not goin\u0027 back?!\nBut why, goro?",
          "TranslatedText": null
        }
      ]
    },
    {
      "EntryName": "talk156",
      "TextList": [
        {
          "Line": 768,
          "OriginalText": "WHAT?! ",
          "TranslatedText": null
        },
        {
          "Line": 772,
          "OriginalText": "You\u0027re gonna try to board\nthe Divine Beast?\n\nWho do you think you are...",
          "TranslatedText": null
        },
        {
          "Line": 776,
          "OriginalText": "Lord Daruk",
          "TranslatedText": null
        },
        {
          "Line": 779,
          "OriginalText": "?\nIt\u0027s dangerous in there, goro!\n\n",
          "TranslatedText": null
        },
        {
          "Line": 788,
          "OriginalText": "Though I guess if you weakened it first,\nit might be possible...",
          "TranslatedText": null
        },
        {
          "Line": 792,
          "OriginalText": "\n\nBut...",
          "TranslatedText": null
        },
        {
          "Line": 796,
          "OriginalText": "why risk it?!",
          "TranslatedText": null
        }
      ]
    },
    {
      "EntryName": "talk158",
      "TextList": [
        {
          "Line": 805,
          "OriginalText": "Hmm. ",
          "TranslatedText": null
        },
        {
          "Line": 809,
          "OriginalText": "Well, I don\u0027t really get it, ",
          "TranslatedText": null
        },
        {
          "Line": 813,
          "OriginalText": "but I\u0027m\nsure you have your reasons, goro...",
          "TranslatedText": null
        }
      ]
    },
    {
      "EntryName": "talk159",
      "TextList": [
        {
          "Line": 822,
          "OriginalText": "If ",
          "TranslatedText": null
        },
        {
          "Line": 826,
          "OriginalText": "Boss ",
          "TranslatedText": null
        },
        {
          "Line": 829,
          "OriginalText": "can\u0027t come down here, then he\ncan\u0027t use the cannon to lower the bridge.",
          "TranslatedText": null
        },
        {
          "Line": 833,
          "OriginalText": "\nThat means we\u0027re out of luck, goro!",
          "TranslatedText": null
        }
      ]
    },
    {
      "EntryName": "talk16",
      "TextList": [
        {
          "Line": 842,
          "OriginalText": "OK! Here we go!",
          "TranslatedText": null
        },
        {
          "Line": 846,
          "OriginalText": "\n",
          "TranslatedText": null
        },
        {
          "Line": 850,
          "OriginalText": "Gosh, I\u0027m nervous...goro.",
          "TranslatedText": null
        }
      ]
    },
    {
      "EntryName": "talk160",
      "TextList": [
        {
          "Line": 862,
          "OriginalText": "Oh! ",
          "TranslatedText": null
        },
        {
          "Line": 866,
          "OriginalText": "It occurs to me that YOU know how\nto use the cannons! Don\u0027t you, goro?\n\n",
          "TranslatedText": null
        },
        {
          "Line": 875,
          "OriginalText": "Maybe we CAN get across after all...",
          "TranslatedText": null
        }
      ]
    },
    {
      "EntryName": "talk162",
      "TextList": [
        {
          "Line": 887,
          "OriginalText": "So let\u0027s just do what I do to help ",
          "TranslatedText": null
        },
        {
          "Line": 891,
          "OriginalText": "Boss",
          "TranslatedText": null
        },
        {
          "Line": 894,
          "OriginalText": "\nsometimes. That means I\u0027m gonna be the\ncannonball! ",
          "TranslatedText": null
        },
        {
          "Line": 898,
          "OriginalText": "And you\u0027ll launch me, goro!",
          "TranslatedText": null
        }
      ]
    },
    {
      "EntryName": "talk163",
      "TextList": [
        {
          "Line": 910,
          "OriginalText": "Thanks for savin\u0027 me, brother!",
          "TranslatedText": null
        },
        {
          "Line": 914,
          "OriginalText": " I would\u0027ve\nbeen in real trouble if you hadn\u0027t shown\nup when you did, goro.\n",
          "TranslatedText": null
        },
        {
          "Line": 920,
          "OriginalText": "*sigh*",
          "TranslatedText": null
        },
        {
          "Line": 924,
          "OriginalText": " Still no sign of Boss, eh?",
          "TranslatedText": null
        }
      ]
    },
    {
      "EntryName": "talk164",
      "TextList": [
        {
          "Line": 936,
          "OriginalText": "Huh? ",
          "TranslatedText": null
        },
        {
          "Line": 940,
          "OriginalText": "Not coming?",
          "TranslatedText": null
        },
        {
          "Line": 944,
          "OriginalText": " But why, goro?!",
          "TranslatedText": null
        }
      ]
    },
    {
      "EntryName": "talk165",
      "TextList": [
        {
          "Line": 956,
          "OriginalText": "I\u0027ve got ",
          "TranslatedText": null
        },
        {
          "Line": 960,
          "OriginalText": "Daruk\u0027s Protection",
          "TranslatedText": null
        },
        {
          "Line": 963,
          "OriginalText": ", goro!\nMy ancestor\u0027s help can\u0027t possibly\nfail me!\nThat means I can smash into anything\nand it won\u0027t hurt one bit, goro!",
          "TranslatedText": null
        }
      ]
    },
    {
      "EntryName": "talk166",
      "TextList": [
        {
          "Line": 972,
          "OriginalText": "My ancestor, ",
          "TranslatedText": null
        },
        {
          "Line": 976,
          "OriginalText": "Daruk",
          "TranslatedText": null
        },
        {
          "Line": 979,
          "OriginalText": ", was a big deal, goro.\nThat\u0027s what ",
          "TranslatedText": null
        },
        {
          "Line": 983,
          "OriginalText": "Boss ",
          "TranslatedText": null
        },
        {
          "Line": 986,
          "OriginalText": "says, so it\u0027s just gotta\nbe true!\nAnd I can use ",
          "TranslatedText": null
        },
        {
          "Line": 990,
          "OriginalText": "Daruk\u0027s Protection",
          "TranslatedText": null
        },
        {
          "Line": 993,
          "OriginalText": " just like\n",
          "TranslatedText": null
        },
        {
          "Line": 997,
          "OriginalText": "Daruk",
          "TranslatedText": null
        },
        {
          "Line": 1000,
          "OriginalText": " could! ",
          "TranslatedText": null
        },
        {
          "Line": 1004,
          "OriginalText": "That means there\u0027s no need\nto worry, goro!",
          "TranslatedText": null
        }
      ]
    },
    {
      "EntryName": "talk167",
      "TextList": [
        {
          "Line": 1013,
          "OriginalText": "So rest easy, got it?",
          "TranslatedText": null
        },
        {
          "Line": 1017,
          "OriginalText": " Well, go on then!\nTake aim at the ",
          "TranslatedText": null
        },
        {
          "Line": 1021,
          "OriginalText": "Bridge of Eldin ",
          "TranslatedText": null
        },
        {
          "Line": 1024,
          "OriginalText": "and\nfire away, goro!\n",
          "TranslatedText": null
        },
        {
          "Line": 1028,
          "OriginalText": "Just, y\u0027know...be kinda gentle about it...",
          "TranslatedText": null
        }
      ]
    },
    {
      "EntryName": "talk168",
      "TextList": [
        {
          "Line": 1046,
          "OriginalText": "Owww... ",
          "TranslatedText": null
        },
        {
          "Line": 1050,
          "OriginalText": "OUCH...",
          "TranslatedText": null
        }
      ]
    },
    {
      "EntryName": "talk169",
      "TextList": [
        {
          "Line": 1065,
          "OriginalText": "Take \u0027em out!",
          "TranslatedText": null
        }
      ]
    },
    {
      "EntryName": "talk17",
      "TextList": [
        {
          "Line": 1077,
          "OriginalText": "The ",
          "TranslatedText": null
        },
        {
          "Line": 1081,
          "OriginalText": "Divine Beast Vah Rudania",
          "TranslatedText": null
        },
        {
          "Line": 1084,
          "OriginalText": "...",
          "TranslatedText": null
        },
        {
          "Line": 1088,
          "OriginalText": "\nis out causing trouble again.",
          "TranslatedText": null
        }
      ]
    },
    {
      "EntryName": "talk170",
      "TextList": [
        {
          "Line": 1100,
          "OriginalText": "Eeeeek!",
          "TranslatedText": null
        }
      ]
    },
    {
      "EntryName": "talk171",
      "TextList": [
        {
          "Line": 1112,
          "OriginalText": "Ya know...",
          "TranslatedText": null
        },
        {
          "Line": 1116,
          "OriginalText": " While I was up there,",
          "TranslatedText": null
        },
        {
          "Line": 1120,
          "OriginalText": " I coulda\nsworn I saw ",
          "TranslatedText": null
        },
        {
          "Line": 1124,
          "OriginalText": "Lord Daruk ",
          "TranslatedText": null
        },
        {
          "Line": 1127,
          "OriginalText": "standin\u0027 on top\nof ",
          "TranslatedText": null
        },
        {
          "Line": 1131,
          "OriginalText": "Rudania",
          "TranslatedText": null
        },
        {
          "Line": 1134,
          "OriginalText": "!\n",
          "TranslatedText": null
        },
        {
          "Line": 1140,
          "OriginalText": "I wonder...",
          "TranslatedText": null
        },
        {
          "Line": 1144,
          "OriginalText": " Was that his ghost?!\n\n\n",
          "TranslatedText": null
        },
        {
          "Line": 1150,
          "OriginalText": "I dunno how to explain it, but somehow\nI feel a lot braver now, goro!",
          "TranslatedText": null
        }
      ]
    },
    {
      "EntryName": "talk172",
      "TextList": [
        {
          "Line": 1159,
          "OriginalText": "I\u0027ll follow your instructions\u2014promise!",
          "TranslatedText": null
        },
        {
          "Line": 1163,
          "OriginalText": "\nJust ",
          "TranslatedText": null
        },
        {
          "Line": 1167,
          "OriginalText": "whistle ",
          "TranslatedText": null
        },
        {
          "Line": 1170,
          "OriginalText": "to let me know what\nI need to do, goro.",
          "TranslatedText": null
        }
      ]
    },
    {
      "EntryName": "talk173",
      "TextList": [
        {
          "Line": 1179,
          "OriginalText": "Each time you ",
          "TranslatedText": null
        },
        {
          "Line": 1183,
          "OriginalText": "whistle",
          "TranslatedText": null
        },
        {
          "Line": 1186,
          "OriginalText": ",",
          "TranslatedText": null
        },
        {
          "Line": 1190,
          "OriginalText": " I\u0027ll either stop or\nstart moving accordingly. OK?",
          "TranslatedText": null
        }
      ]
    },
    {
      "EntryName": "talk18",
      "TextList": [
        {
          "Line": 1199,
          "OriginalText": "The ",
          "TranslatedText": null
        },
        {
          "Line": 1203,
          "OriginalText": "Bridge of Eldin ",
          "TranslatedText": null
        },
        {
          "Line": 1206,
          "OriginalText": "is down...",
          "TranslatedText": null
        },
        {
          "Line": 1210,
          "OriginalText": "\nOK! ",
          "TranslatedText": null
        },
        {
          "Line": 1214,
          "OriginalText": "Let\u0027s get across quickly, goro!",
          "TranslatedText": null
        }
      ]
    },
    {
      "EntryName": "talk19",
      "TextList": [
        {
          "Line": 1223,
          "OriginalText": "My ancestor\u0027s power, ",
          "TranslatedText": null
        },
        {
          "Line": 1227,
          "OriginalText": "Daruk\u0027s Protection",
          "TranslatedText": null
        },
        {
          "Line": 1230,
          "OriginalText": ",\nis amazing! ",
          "TranslatedText": null
        },
        {
          "Line": 1234,
          "OriginalText": "OK then. ",
          "TranslatedText": null
        },
        {
          "Line": 1238,
          "OriginalText": "Let\u0027s get across\nthat bridge quickly, goro!",
          "TranslatedText": null
        }
      ]
    },
    {
      "EntryName": "talk49",
      "TextList": [
        {
          "Line": 1247,
          "OriginalText": "OK, I\u0027ll leave the rest to you.",
          "TranslatedText": null
        },
        {
          "Line": 1251,
          "OriginalText": "\nI\u0027ll be watching from here, goro.\n\nIf you feel like you need to prepare a bit\nmore,",
          "TranslatedText": null
        },
        {
          "Line": 1255,
          "OriginalText": " you can always return to town.",
          "TranslatedText": null
        }
      ]
    },
    {
      "EntryName": "talk51",
      "TextList": [
        {
          "Line": 1264,
          "OriginalText": "Hey, it\u0027s you!",
          "TranslatedText": null
        },
        {
          "Line": 1268,
          "OriginalText": " But... Wait... Didn\u0027t you\njump down? I coulda sworn you did...\n\n",
          "TranslatedText": null
        },
        {
          "Line": 1274,
          "OriginalText": "Anyway, ",
          "TranslatedText": null
        },
        {
          "Line": 1278,
          "OriginalText": "it looks like ",
          "TranslatedText": null
        },
        {
          "Line": 1282,
          "OriginalText": "Rudania ",
          "TranslatedText": null
        },
        {
          "Line": 1285,
          "OriginalText": "is still\nactive down there.\n\nI really don\u0027t want him gettin\u0027 back up,\ngoro. We\u0027ve gotta do somethin\u0027!\n\nBut there\u0027s no way I\u0027d survive if I jumped\ndown from this high up, goro...",
          "TranslatedText": null
        }
      ]
    },
    {
      "EntryName": "talk52",
      "TextList": [
        {
          "Line": 1294,
          "OriginalText": "Hey! ",
          "TranslatedText": null
        },
        {
          "Line": 1298,
          "OriginalText": "Brother!",
          "TranslatedText": null
        },
        {
          "Line": 1302,
          "OriginalText": " Thanks so much for\nappeasing ",
          "TranslatedText": null
        },
        {
          "Line": 1306,
          "OriginalText": "Rudania",
          "TranslatedText": null
        },
        {
          "Line": 1309,
          "OriginalText": ", goro!",
          "TranslatedText": null
        }
      ]
    },
    {
      "EntryName": "talk53",
      "TextList": [
        {
          "Line": 1321,
          "OriginalText": "Oh! ",
          "TranslatedText": null
        },
        {
          "Line": 1325,
          "OriginalText": "That\u0027s right!",
          "TranslatedText": null
        },
        {
          "Line": 1329,
          "OriginalText": "\n\n\n",
          "TranslatedText": null
        },
        {
          "Line": 1335,
          "OriginalText": "I talked to ",
          "TranslatedText": null
        },
        {
          "Line": 1339,
          "OriginalText": "Boss ",
          "TranslatedText": null
        },
        {
          "Line": 1342,
          "OriginalText": "about you,",
          "TranslatedText": null
        },
        {
          "Line": 1346,
          "OriginalText": " and he said\nto bring you to him!\n\nAn order from ",
          "TranslatedText": null
        },
        {
          "Line": 1350,
          "OriginalText": "Boss",
          "TranslatedText": null
        },
        {
          "Line": 1353,
          "OriginalText": " can\u0027t be refused.",
          "TranslatedText": null
        },
        {
          "Line": 1357,
          "OriginalText": "\nMove it, goro!",
          "TranslatedText": null
        }
      ]
    },
    {
      "EntryName": "talk58",
      "TextList": [
        {
          "Line": 1366,
          "OriginalText": "I don\u0027t exactly know what you were doin\u0027\nin ",
          "TranslatedText": null
        },
        {
          "Line": 1370,
          "OriginalText": "Rudania",
          "TranslatedText": null
        },
        {
          "Line": 1373,
          "OriginalText": ", but...\n\nWell...",
          "TranslatedText": null
        },
        {
          "Line": 1377,
          "OriginalText": "it seems to have calmed down,\ngoro!",
          "TranslatedText": null
        }
      ]
    },
    {
      "EntryName": "talk66",
      "TextList": [
        {
          "Line": 1386,
          "OriginalText": "Let\u0027s hurry and cross the bridge, goro!",
          "TranslatedText": null
        }
      ]
    },
    {
      "EntryName": "talk94",
      "TextList": [
        {
          "Line": 1398,
          "OriginalText": "Oh...",
          "TranslatedText": null
        },
        {
          "Line": 1402,
          "OriginalText": " Yikes...",
          "TranslatedText": null
        },
        {
          "Line": 1406,
          "OriginalText": " A ",
          "TranslatedText": null
        },
        {
          "Line": 1410,
          "OriginalText": "sentry ",
          "TranslatedText": null
        },
        {
          "Line": 1413,
          "OriginalText": "is always a\nnuisance, no matter where it is or\nhow many of them there are.\nIf one finds you, ",
          "TranslatedText": null
        },
        {
          "Line": 1417,
          "OriginalText": "Rudania ",
          "TranslatedText": null
        },
        {
          "Line": 1420,
          "OriginalText": "will go nuts,",
          "TranslatedText": null
        },
        {
          "Line": 1424,
          "OriginalText": "\nwhich will cause ",
          "TranslatedText": null
        },
        {
          "Line": 1428,
          "OriginalText": "Death Mountain",
          "TranslatedText": null
        },
        {
          "Line": 1431,
          "OriginalText": "\nto erupt, goro.\n",
          "TranslatedText": null
        },
        {
          "Line": 1437,
          "OriginalText": "I...",
          "TranslatedText": null
        },
        {
          "Line": 1441,
          "OriginalText": "always get spotted,",
          "TranslatedText": null
        },
        {
          "Line": 1445,
          "OriginalText": " and then\n",
          "TranslatedText": null
        },
        {
          "Line": 1449,
          "OriginalText": "magma bombs",
          "TranslatedText": null
        },
        {
          "Line": 1452,
          "OriginalText": " come raining down\none after another.\nIf ",
          "TranslatedText": null
        },
        {
          "Line": 1456,
          "OriginalText": "Boss",
          "TranslatedText": null
        },
        {
          "Line": 1459,
          "OriginalText": " were here, he\u0027d swat those\n",
          "TranslatedText": null
        },
        {
          "Line": 1463,
          "OriginalText": "magma bombs ",
          "TranslatedText": null
        },
        {
          "Line": 1466,
          "OriginalText": "away like they were\nnothin\u0027 and force his way up!\nI don\u0027t think you\u0027d be able to do that,\ngoro. And I don\u0027t wanna cause you\nany trouble. ",
          "TranslatedText": null
        },
        {
          "Line": 1470,
          "OriginalText": "Hmmm...\nMaybe the best idea is for you to give me\na ",
          "TranslatedText": null
        },
        {
          "Line": 1474,
          "OriginalText": "signal ",
          "TranslatedText": null
        },
        {
          "Line": 1477,
          "OriginalText": "to let me know when I should\nstop and when I can move forward.",
          "TranslatedText": null
        }
      ]
    },
    {
      "EntryName": "talk95",
      "TextList": [
        {
          "Line": 1486,
          "OriginalText": "Let\u0027s hurry and decide what the ",
          "TranslatedText": null
        },
        {
          "Line": 1490,
          "OriginalText": "signal",
          "TranslatedText": null
        },
        {
          "Line": 1493,
          "OriginalText": "\nwill be, goro!",
          "TranslatedText": null
        },
        {
          "Line": 1497,
          "OriginalText": " Um, got any ideas?",
          "TranslatedText": null
        }
      ]
    },
    {
      "EntryName": "talk96",
      "TextList": [
        {
          "Line": 1515,
          "OriginalText": "A wink? ",
          "TranslatedText": null
        },
        {
          "Line": 1519,
          "OriginalText": "Ummm...\n\n\n",
          "TranslatedText": null
        },
        {
          "Line": 1525,
          "OriginalText": "Yeaaaah, I don\u0027t think I\u0027d be able to see\nthat from way off in the distance, goro.",
          "TranslatedText": null
        },
        {
          "Line": 1529,
          "OriginalText": "\nTry to think of somethin\u0027 better!",
          "TranslatedText": null
        }
      ]
    },
    {
      "EntryName": "talk98",
      "TextList": [
        {
          "Line": 1538,
          "OriginalText": "Mind powers?! Wow!",
          "TranslatedText": null
        },
        {
          "Line": 1542,
          "OriginalText": " You mean, like,\nreal-life telepathy?\n\n",
          "TranslatedText": null
        },
        {
          "Line": 1548,
          "OriginalText": "I didn\u0027t know you ",
          "TranslatedText": null
        },
        {
          "Line": 1552,
          "OriginalText": "Hylians ",
          "TranslatedText": null
        },
        {
          "Line": 1555,
          "OriginalText": "could do that\nsort of thing these days!\n\n",
          "TranslatedText": null
        },
        {
          "Line": 1567,
          "OriginalText": "...",
          "TranslatedText": null
        },
        {
          "Line": 1571,
          "OriginalText": "\n",
          "TranslatedText": null
        },
        {
          "Line": 1578,
          "OriginalText": "Are you transmitting yet, goro?",
          "TranslatedText": null
        },
        {
          "Line": 1587,
          "OriginalText": "\n...\n",
          "TranslatedText": null
        },
        {
          "Line": 1593,
          "OriginalText": "What the? ",
          "TranslatedText": null
        },
        {
          "Line": 1597,
          "OriginalText": "I\u0027m not receiving anything\nover here!",
          "TranslatedText": null
        }
      ]
    },
    {
      "EntryName": "talk99",
      "TextList": [
        {
          "Line": 1606,
          "OriginalText": "A whistle? ",
          "TranslatedText": null
        },
        {
          "Line": 1610,
          "OriginalText": "Ah, that\u0027s a great idea, goro!\n\n\n",
          "TranslatedText": null
        },
        {
          "Line": 1616,
          "OriginalText": "When I hear your whistle,",
          "TranslatedText": null
        },
        {
          "Line": 1620,
          "OriginalText": " I\u0027ll either stop\nin my tracks or start moving forward!\nI like it!\n",
          "TranslatedText": null
        },
        {
          "Line": 1629,
          "OriginalText": "Even though it\u0027s kinda like calling a horse, goro...",
          "TranslatedText": null
        },
        {
          "Line": 1638,
          "OriginalText": "\n\n\n",
          "TranslatedText": null
        },
        {
          "Line": 1644,
          "OriginalText": "OK then. ",
          "TranslatedText": null
        },
        {
          "Line": 1648,
          "OriginalText": "I\u0027m counting on you and your\nwhistles",
          "TranslatedText": null
        },
        {
          "Line": 1652,
          "OriginalText": " to make sure I\u0027m not spotted\nby those awful ",
          "TranslatedText": null
        },
        {
          "Line": 1656,
          "OriginalText": "sentries",
          "TranslatedText": null
        },
        {
          "Line": 1659,
          "OriginalText": "!",
          "TranslatedText": null
        }
      ]
    }
  ]
