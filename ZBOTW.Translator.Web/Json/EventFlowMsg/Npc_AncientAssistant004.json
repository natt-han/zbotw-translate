      "NPC": "Jerrin",
      "TextList": [
        {
          "Line": 587,
          "OriginalText": "Link, ",
          "TranslatedText": "Link, ",
          "Length": 6
        },
        {
          "Line": 591,
          "OriginalText": "check out the ",
          "TranslatedText": "ลองไปดูที่",
          "Length": 14
        },
        {
          "Line": 595,
          "OriginalText": "left eye of Skull Lake",
          "TranslatedText": "ตาซ้ายของ Skull Lake ",
          "Colour": "red",
          "Length": 22
        },
        {
          "Line": 598,
          "OriginalText": ".",
          "TranslatedText": "สิ",
          "Length": 1
        }
      ]
    },
    {
      "EntryName": "talk30",
      "NPC": "Jerrin",
      "TextList": [
        {
          "Line": 607,
          "OriginalText": "I know there\u0027s a ",
          "TranslatedText": "ฉันรู้ว่ามี",
          "Length": 17
        },
        {
          "Line": 611,
          "OriginalText": "shrine",
