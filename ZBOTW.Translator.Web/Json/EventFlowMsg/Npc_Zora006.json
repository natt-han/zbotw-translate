        {
          "Line": 196,
          "OriginalText": "Ruta",
          "TranslatedText": null,
          "Colour": "blue"
        },
        {
          "Line": 199,
          "OriginalText": " has settled down,",
          "TranslatedText": null,
          "Colour": null
