      "NPC": "Shabonne",
      "TextList": [
        {
          "Line": 436,
          "OriginalText": "What? What do you need?",
          "TranslatedText": null,
          "Length": 23
        },
        {
          "Line": 440,
          "OriginalText": " You better not\nhave come here to mock Tali now that\nyou\u0027re the new champion racer!",
          "TranslatedText": null,
          "Length": 83
