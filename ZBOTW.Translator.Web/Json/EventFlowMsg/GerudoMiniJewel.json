    {
      "EntryName": "Talk_02",
      "TextList": [
        {
          "Line": 74,
          "OriginalText": "Sarqso!",
          "TranslatedText": null
        },
        {
          "Line": 78,
          "OriginalText": "\n\n\n",
          "TranslatedText": null
        },
        {
          "Line": 84,
          "OriginalText": "But...I can\u0027t take this all from you with\njust a sarqso. We hardly know each\nother,",
          "TranslatedText": null
        },
        {
          "Line": 88,
          "OriginalText": " and you\u0027ve been so nice...\n",
          "TranslatedText": null
        },
        {
          "Line": 94,
          "OriginalText": "The shop\u0027s been closed so long,\nI hardly have any inventory...",
          "TranslatedText": null
        },
        {
          "Line": 98,
          "OriginalText": "\n\n",
          "TranslatedText": null
        },
        {
          "Line": 109,
          "OriginalText": "Oh, of course!",
          "TranslatedText": null
        },
        {
          "Line": 113,
          "OriginalText": " ",
          "TranslatedText": null
        },
        {
          "Line": 124,
          "OriginalText": "Now that I have some\n",
          "TranslatedText": null
