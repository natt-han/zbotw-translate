    {
      "EntryName": "talk07",
      "TextList": [
        {
          "Line": 190,
          "OriginalText": "I have to get ready for bed.",
          "TranslatedText": null
        },
        {
          "Line": 194,
          "OriginalText": "\nYou should do the same.",
          "TranslatedText": null
        }
      ]
    },
    {
      "EntryName": "talk08",
      "TextList": [
        {
          "Line": 203,
          "OriginalText": "Now then, I\u0027ve got to get to bed.",
          "TranslatedText": null
        },
        {
          "Line": 207,
          "OriginalText": "\nIt\u0027s rough on my body if I don\u0027t get a\ngood night\u0027s rest.",
          "TranslatedText": null
        }
      ]
    },
    {
      "EntryName": "talk09",
      "TextList": [
        {
          "Line": 216,
          "OriginalText": "Oh... ",
          "TranslatedText": null
        },
        {
          "Line": 220,
          "OriginalText": "Is that so?",
          "TranslatedText": null
        }
      ]
