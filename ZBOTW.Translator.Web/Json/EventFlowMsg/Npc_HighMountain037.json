      "TextList": [
        {
          "Line": 749,
          "OriginalText": "Medoh ",
          "TranslatedText": null,
          "Colour": "blue"
        },
        {
          "Line": 752,
          "OriginalText": "is calm now, so Rito Village\nshould be at peace.",
