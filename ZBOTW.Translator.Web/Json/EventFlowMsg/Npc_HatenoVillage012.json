      ]
    },
    {
      "EntryName": "talk02",
      "TextList": [
        {
          "Line": 109,
          "OriginalText": "Say, isn\u0027t that ",
          "TranslatedText": null
        },
        {
          "Line": 113,
          "OriginalText": "blue flame",
          "TranslatedText": null
        },
        {
          "Line": 116,
          "OriginalText": "...",
          "TranslatedText": null
        },
        {
          "Line": 120,
          "OriginalText": " \nIsn\u0027t that Grandma Purah\u0027s?\n\n",
          "TranslatedText": null
        },
        {
          "Line": 129,
          "OriginalText": "Her house ",
          "TranslatedText": null
        },
        {
          "Line": 132,
          "OriginalText": "isn\u0027t near here.",
          "TranslatedText": null
        },
        {
          "Line": 136,
          "OriginalText": " It\u0027s way on\nthe eastern edge of the village.",
          "TranslatedText": null
        }
      ]
    },
    {
      "EntryName": "talk03",
      "TextList": [
        {
          "Line": 145,
          "OriginalText": "Well, see ya.",
          "TranslatedText": null
        }
      ]
    },
    {
      "EntryName": "talk04",
      "TextList": [
        {
          "Line": 154,
          "OriginalText": "Yeah, ",
          "TranslatedText": null
        },
        {
          "Line": 158,
          "OriginalText": "Mount Lanayru",
          "TranslatedText": null
        },
        {
          "Line": 161,
          "OriginalText": "\u0027s interesting...",
          "TranslatedText": null
        },
        {
          "Line": 165,
          "OriginalText": "\n\n\nThe ",
          "TranslatedText": null
        },
        {
          "Line": 172,
          "OriginalText": "Spring of Wisdom",
          "TranslatedText": null
        },
        {
          "Line": 176,
          "OriginalText": " ",
          "TranslatedText": null
        },
        {
          "Line": 179,
          "OriginalText": "the princess of \n",
          "TranslatedText": null
