    {
      "EntryName": "talk01",
      "TextList": [
        {
          "Line": 89,
          "OriginalText": "Well... ",
          "TranslatedText": null
        },
        {
          "Line": 93,
          "OriginalText": "\u0022Nothing\u0022 might be going a little\nbit overboard...\n\nWe do have a ",
          "TranslatedText": null
        },
        {
          "Line": 97,
          "OriginalText": "dye shop",
          "TranslatedText": null
        },
        {
          "Line": 100,
          "OriginalText": ". I don\u0027t think \nthey have one of those in other towns.",
          "TranslatedText": null
