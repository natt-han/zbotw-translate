      ]
    },
    {
      "EntryName": "0013",
      "TextList": [
        {
          "Line": 52,
          "OriginalText": "You don\u0027t have to.",
          "TranslatedText": "ไม่ต้องหรอก"
        }
      ]
    },
    {
      "EntryName": "0014",
      "TextList": [
        {
          "Line": 56,
          "OriginalText": "Yes.",
          "TranslatedText": "ใช่"
        }
      ]
    },
    {
      "EntryName": "0015",
      "TextList": [
        {
          "Line": 60,
          "OriginalText": "No.",
          "TranslatedText": "ไม่"
        }
      ]
    },
    {
      "EntryName": "0016",
      "TextList": [
        {
          "Line": 64,
          "OriginalText": "Rupees?",
          "TranslatedText": "Rupees?"
        }
      ]
    },
    {
      "EntryName": "0017",
      "TextList": [
        {
          "Line": 68,
          "OriginalText": "That\u0027s right!",
          "TranslatedText": "ใช่แล้ว!"
        }
      ]
    },
    {
      "EntryName": "0018",
      "TextList": [
        {
          "Line": 72,
          "OriginalText": "Who are you?",
          "TranslatedText": "คุณเป็นใคร?"
        }
      ]
    },
    {
      "EntryName": "0019",
      "TextList": [
        {
          "Line": 76,
          "OriginalText": "Purah got younger.",
          "TranslatedText": "Purah เด็กลง"
        }
      ]
    },
    {
      "EntryName": "near00",
      "TextList": [
        {
          "Line": 80,
          "OriginalText": "Busy, busy...",
          "TranslatedText": "ยุ่งจัง ยุ่งจัง..."
