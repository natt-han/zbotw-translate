    },
    {
      "EntryName": "0006",
      "TextList": [
        {
          "Line": 28,
          "OriginalText": "Let\u0027s do it!",
          "TranslatedText": "มาทำกันเลย!"
        }
      ]
    },
    {
      "EntryName": "0008",
      "TextList": [
        {
          "Line": 32,
          "OriginalText": "Ancient soldier gear?",
          "TranslatedText": "Ancient soldier gear?"
        }
      ]
    },
    {
      "EntryName": "0009",
      "TextList": [
