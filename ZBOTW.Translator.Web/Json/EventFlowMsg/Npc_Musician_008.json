      "Choices": [
        5,
        6
      ],
      "TextList": [
        {
          "Line": 319,
          "OriginalText": "Th-",
          "TranslatedText": "-",
          "Length": 3
        },
        {
          "Line": 323,
          "OriginalText": "that there...",
          "TranslatedText": "ที่นั่น .....",
          "Length": 13
        },
        {
          "Line": 327,
          "OriginalText": "on your hip!",
          "TranslatedText": "ที่สะโพกของคุณ!!",
          "Length": 12
        },
        {
          "Line": 341,
          "OriginalText": "...",
          "TranslatedText": "...",
          "Length": 3
        },
        {
          "Line": 345,
          "OriginalText": "No, I\u0027m sorry—",
          "TranslatedText": "ไม่ฉันขอโทษ—",
          "Length": 14
        },
        {
          "Line": 349,
          "OriginalText": "it\u0027s nothing.",
          "TranslatedText": "ไม่มีอะไรเลย .",
          "Length": 13
        },
        {
          "Line": 353,
          "OriginalText": "\nI didn\u0027t mean to pry.",
          "TranslatedText": "\nฉันไม่ได้ตั้งใจแงะ.",
          "Length": 22
        }
      ]
    },
    {
      "EntryName": "Talk16",
      "TextList": [
        {
          "Line": 370,
          "OriginalText": "Allow me to introduce myself.",
          "TranslatedText": "อนุญาตให้ฉันแนะนำตัวเอง.",
          "Length": 29
        },
        {
          "Line": 374,
          "OriginalText": "\nMy name is Kass.",
          "TranslatedText": "\nฉันชื่อ Kass.",
          "Length": 17
        },
        {
          "Line": 378,
          "OriginalText": " I\u0027m a bard traveling\nthis land in search of ancient songs.",
          "TranslatedText": " \u00A0 ฉันกวีที่เดินทาง\nไปดินแดนนี้ในการค้นหาเพลงโบราณ",
          "Length": 59
        }
      ]
    },
