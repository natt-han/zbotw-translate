    {
      "EntryName": "MSG_Akka_00",
      "NPC": "Laroba",
      "TextList": [
        {
          "Line": 81,
          "OriginalText": "Hey, kid! ",
          "TranslatedText": null
        },
        {
          "Line": 85,
          "OriginalText": "Are you trying to get\nsomeplace? ",
          "TranslatedText": null
        },
        {
          "Line": 89,
          "OriginalText": "Do you have a map?",
          "TranslatedText": null
        },
        {
          "Line": 95,
          "OriginalText": "If you do, then it\u0027s good to set a goal to\nreach and then check your map every\nso often to get your bearings.\n",
          "TranslatedText": null
        },
        {
          "Line": 101,
          "OriginalText": "Take me, for instance—",
          "TranslatedText": null
        },
        {
          "Line": 105,
          "OriginalText": "my goal is to find\na fine, strapping voe! ",
          "TranslatedText": null
        },
        {
          "Line": 109,
          "OriginalText": "Best of luck with\nwhatever your goal is, kid!",
          "TranslatedText": null
        }
      ]
    },
    {
      "EntryName": "MSG_Akka_00_1",
      "NPC": "Laroba",
      "TextList": [
        {
