      "EntryName": "Rest_02",
      "NPC": "Laroba",
      "TextList": [
        {
          "Line": 355,
          "OriginalText": "The rain helps cool me off when I get\ntoo flushed from staring at voe.",
          "TranslatedText": null
        }
      ]
    },
