      "EntryName": "New_13",
      "TextList": [
        {
          "Line": 357,
          "OriginalText": "What\u0027s wrong?",
          "TranslatedText": null,
          "Colour": null
        },
        {
          "Line": 361,
          "OriginalText": " Why aren\u0027t you flying?\n",
          "TranslatedText": null,
          "Colour": null
        },
        {
