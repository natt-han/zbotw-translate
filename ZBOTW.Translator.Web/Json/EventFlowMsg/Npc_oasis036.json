        {
          "Line": 700,
          "OriginalText": "chief",
          "TranslatedText": null,
          "Colour": "blue",
          "Length": 5
        },
        {
          "Line": 703,
          "OriginalText": "\u0027s way! Watch out!",
          "TranslatedText": null,
          "Length": 18
        }
      ]
    },
