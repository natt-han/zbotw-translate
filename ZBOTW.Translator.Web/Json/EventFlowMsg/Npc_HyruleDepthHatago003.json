      "NPC": "Trott",
      "TextList": [
        {
          "Line": 65,
          "OriginalText": "Thanks!",
          "TranslatedText": null,
          "Length": 7
        },
        {
          "Line": 69,
          "OriginalText": " Hey, wait a tick... You don\u0027t have\nany raw gourmet meat!\n\n",
          "TranslatedText": null,
          "Length": 59
        },
        {
          "Line": 75,
          "OriginalText": "You got my hopes up for nothing...\nNot cool, man. Talk to me when you\nactually have some raw gourmet meat.",
