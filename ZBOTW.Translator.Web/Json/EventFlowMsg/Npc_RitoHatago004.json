    },
    {
      "EntryName": "Register19",
      "NPC": "Galli",
      "TextList": [
        {
          "Line": 936,
          "OriginalText": "But your horse isn\u0027t magic. It can\u0027t hear\nyou if you\u0027re too far away, so do keep\nthat in mind!",
          "TranslatedText": null,
          "Length": 94
        }
      ]
