      "Choices": [
        42,
        43
      ],
      "TextList": [
        {
          "Line": 945,
          "OriginalText": "OK,",
          "TranslatedText": null,
          "Length": 3
        },
        {
          "Line": 949,
          "OriginalText": " please enter a name for your hors—\n\n\n",
          "TranslatedText": null,
          "Length": 38
        },
        {
          "Line": 955,
          "OriginalText": "...Wait a minute!\n\n\nIsn\u0027t this ",
          "TranslatedText": null,
          "Length": 31
        },
        {
          "Line": 959,
          "OriginalText": "Epona",
          "TranslatedText": null,
          "Colour": "red",
          "Length": 5
        },
        {
          "Line": 962,
          "OriginalText": ", the horse of legend?\n\n\n",
          "TranslatedText": null,
          "Length": 25
        },
        {
          "Line": 968,
          "OriginalText": "What are you thinking?!\nYou can\u0027t rename a horse of legend!\nPretty sure there\u0027s a law against that.\n",
          "TranslatedText": null,
          "Length": 100
        },
        {
          "Line": 974,
          "OriginalText": "So ",
          "TranslatedText": null,
          "Length": 3
        },
