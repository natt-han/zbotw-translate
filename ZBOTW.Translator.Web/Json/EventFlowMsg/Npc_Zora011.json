    {
      "EntryName": "talk07",
      "NPC": "Tumbo",
      "TextList": [
        {
          "Line": 247,
          "OriginalText": "Hy...leon?",
          "TranslatedText": null
        },
        {
          "Line": 251,
          "OriginalText": " Never met one of\nthose before!",
