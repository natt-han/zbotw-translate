    {
      "EntryName": "talk18",
      "NPC": "Tumbo",
      "TextList": [
        {
          "Line": 538,
          "OriginalText": "Yep!",
          "TranslatedText": null
        },
        {
          "Line": 542,
          "OriginalText": " Since Mom hasn\u0027t come back from\nfishing yet...\n\nI\u0027ve been catching ",
          "TranslatedText": null
