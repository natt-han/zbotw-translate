      ]
    },
    {
      "EntryName": "talk16",
      "NPC": "Jiahto",
      "TextList": [
        {
          "Line": 489,
          "OriginalText": "In the few years I have left, my role and\njoy in this world is to preserve Zora\nhistory for future generations...\nIn other words, I\u0027m counting on you!",
          "TranslatedText": null,
          "Length": 150
        }
      ]
    },
    {
      "EntryName": "talk17",
      "NPC": "Jiahto",
      "TextList": [
        {
          "Line": 498,
          "OriginalText": "Hylian Champion Link...",
          "TranslatedText": null,
          "Length": 23
        },
        {
          "Line": 502,
          "OriginalText": "\nWill you hear my request?",
          "TranslatedText": null,
          "Length": 26
        }
