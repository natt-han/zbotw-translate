        {
          "Line": 525,
          "OriginalText": "\nof a ",
          "TranslatedText": null,
          "Length": 6
        },
        {
          "Line": 529,
          "OriginalText": "stone monument you have yet\nto investigate",
          "TranslatedText": null,
          "Colour": "red",
          "Length": 42
        },
        {
          "Line": 532,
          "OriginalText": "...",
