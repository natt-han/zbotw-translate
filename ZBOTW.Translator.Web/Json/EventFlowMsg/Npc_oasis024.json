        }
      ]
    },
    {
      "EntryName": "Npc_oasis024_Near001",
      "TextList": [
        {
          "Line": 279,
          "OriginalText": "Freeze!",
          "TranslatedText": null
        },
        {
          "Line": 283,
          "OriginalText": " Where do you think you\u0027re\ngoing?!",
          "TranslatedText": null
        },
        {
          "Line": 292,
          "OriginalText": " Not into town\u2014that\u0027s for sure!\nNo voe allowed. ",
          "TranslatedText": null
        },
        {
          "Line": 296,
          "OriginalText": "Off with you!",
          "TranslatedText": null
        }
      ]
    },
