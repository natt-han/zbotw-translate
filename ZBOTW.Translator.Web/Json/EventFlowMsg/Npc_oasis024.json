    },
    {
      "EntryName": "Npc_oasis024_003",
      "TextList": [
        {
          "Line": 104,
          "OriginalText": "This is ",
          "TranslatedText": null
        },
        {
          "Line": 108,
          "OriginalText": "Gerudo Town",
          "TranslatedText": null
        },
        {
          "Line": 111,
          "OriginalText": ", Hyrule\u0027s largest\ntrade hub.\n\nOnly ",
          "TranslatedText": null
        },
        {
          "Line": 115,
          "OriginalText": "vai",
          "TranslatedText": null
        },
        {
          "Line": 118,
          "OriginalText": " are permitted entry within the\nwalls, and we\u0027re constantly on alert for\nanyone who looks suspicious.",
          "TranslatedText": null
        }
      ]
    },
    {
      "EntryName": "Npc_oasis024_004",
      "TextList": [
        {
          "Line": 135,
          "OriginalText": "This place is famous for its bustling\ntrade, but also for our unusual rule\nallowing only ",
          "TranslatedText": null
        },
        {
          "Line": 139,
          "OriginalText": "vai",
          "TranslatedText": null
        },
        {
          "Line": 142,
          "OriginalText": " into the town.\nWe\u0027re constantly on the lookout for\nanyone suspicious, so you can rest\neasy here.",
          "TranslatedText": null
        }
      ]
    },
    {
      "EntryName": "Npc_oasis024_005",
      "TextList": [
        {
