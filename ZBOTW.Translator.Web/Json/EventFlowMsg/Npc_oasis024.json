    },
    {
      "EntryName": "Npc_oasis024_002",
      "TextList": [
        {
          "Line": 60,
          "OriginalText": "Sorry, but ",
          "TranslatedText": null
        },
        {
          "Line": 64,
          "OriginalText": "voe",
          "TranslatedText": null
        },
        {
          "Line": 67,
          "OriginalText": " are not allowed in town.\nIf you need to make preparations for\nyour journey, you can do so elsewhere.\n",
          "TranslatedText": null
        },
        {
          "Line": 73,
          "OriginalText": "Try ",
          "TranslatedText": null
        },
        {
          "Line": 77,
          "OriginalText": "Kara Kara Bazaar",
          "TranslatedText": null
        },
        {
